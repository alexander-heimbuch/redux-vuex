{
  "name": "redux-vuex",
  "version": "3.0.0",
  "description": "Redux bindings for VueJS inspired by Vuex.",
  "keywords": [
    "vue",
    "vuejs",
    "redux",
    "vuex",
    "bindings"
  ],
  "author": "Alexander Heimbuch <github@heimbu.ch>",
  "repository": "git@github.com:alexander-heimbuch/redux-vuex.git",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist/*",
    "typings": "tsc",
    "package": "vite build",
    "build": "npm run clean && npm run package && npm run typings",
    "test": "jest \"__tests__/.*\\.test\\.ts\"",
    "release": "release-it",
    "prepare": "husky install"
  },
  "dependencies": {
    "get-value": "3.0.1",
    "set-value": "4.1.0"
  },
  "peerDependencies": {
    "vue": "3.x",
    "redux": "4.x"
  },
  "module": "./dist/redux-vuex.es.js",
  "types": "./dist/typings/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/redux-vuex.es.js"
    }
  },
  "devDependencies": {
    "@vue/test-utils": "2.2.1",
    "@types/jest": "27.0.1",
    "husky": "8.0.1",
    "jest": "26.4.2",
    "prettier": "2.1.2",
    "pretty-quick": "3.0.2",
    "redux": "4.2.0",
    "release-it": "15.5.0",
    "ts-jest": "^26.4.0",
    "typescript": "4.8.4",
    "vue": "3.2.41",
    "vite": "3.1.8",
    "@vitejs/plugin-vue": "3.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "",
    "testURL": "http://localhost/"
  },
  "release-it": {
    "hooks": {
      "before:init": [
        "npm test"
      ],
      "after:bump": [
        "npm run build"
      ]
    },
    "git": {
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": true
    }
  }
}
