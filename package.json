{
  "name": "redux-vuex",
  "version": "4.0.0",
  "description": "Redux bindings for VueJS inspired by Vuex.",
  "type": "module",
  "keywords": [
    "vue",
    "vuejs",
    "redux",
    "vuex",
    "bindings"
  ],
  "author": "Alexander Heimbuch <github@heimbu.ch>",
  "repository": "git@github.com:alexander-heimbuch/redux-vuex.git",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist/*",
    "typings": "tsc",
    "package": "vite build",
    "build": "npm run clean && npm run package && npm run typings",
    "test": "vitest",
    "prepare": "husky install",
    "release": "release-it"
  },
  "peerDependencies": {
    "redux": "5.x",
    "vue": "3.x"
  },
  "module": "./dist/redux-vuex.es.js",
  "main": "./dist/redux-vuex.cjs.js",
  "types": "./dist/typings/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/typings/index.d.ts",
      "import": "./dist/redux-vuex.es.js",
      "require": "./dist/redux-vuex.cjs.js"
    }
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "6.0.1",
    "@vue/test-utils": "2.4.5",
    "happy-dom": "14.7.1",
    "husky": "9.1.7",
    "prettier": "3.2.5",
    "pretty-quick": "4.0.0",
    "redux": "5.0.1",
    "release-it": "19.0.6",
    "typescript": "5.4.5",
    "vite": "7.2.2",
    "vitest": "4.0.10",
    "vue": "3.4.26",
    "@babel/types": "7.24.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "",
    "testURL": "http://localhost/"
  },
  "release-it": {
    "hooks": {
      "before:init": [
        "npm test -- run"
      ],
      "after:bump": [
        "npm run build"
      ]
    },
    "git": {
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": true
    }
  }
}
