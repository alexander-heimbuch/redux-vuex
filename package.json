{
  "name": "redux-vuex",
  "version": "3.0.0-beta.7",
  "description": "Redux bindings for VueJS inspired by Vuex.",
  "keywords": [
    "vue",
    "vuejs",
    "redux",
    "vuex",
    "bindings"
  ],
  "author": "Alexander Heimbuch <github@heimbu.ch>",
  "repository": "git@github.com:alexander-heimbuch/redux-vuex.git",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && webpack && node ./.build/prepublish.js",
    "test": "jest \"__tests__/.*\\.test\\.ts\"",
    "release": "release-it"
  },
  "dependencies": {
    "get-value": "3.0.1",
    "set-value": "3.0.1"
  },
  "peerDependencies": {
    "vue": "3.x",
    "redux": "4.x",
    "@vue/runtime-core": "3.x"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.11.5",
    "@babel/runtime": "7.11.2",
    "@vue/runtime-core": "3.0.0",
    "@vue/test-utils": "2.0.0-beta.5",
    "@types/jest": "27.0.1",
    "husky": "4.3.0",
    "jest": "26.4.2",
    "prettier": "2.1.2",
    "pretty-quick": "3.0.2",
    "redux": "4.1.1",
    "release-it": "14.11.5",
    "ts-jest": "^26.4.0",
    "ts-loader": "8.0.4",
    "typescript": "4.0.3",
    "vue": "3.0.0",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "@types/get-value": "3.0.0",
    "fs-extra": "10.0.0",
    "ramda": "0.27.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "",
    "testURL": "http://localhost/"
  },
  "release-it": {
    "hooks": {
      "before:init": [
        "npm test"
      ],
      "after:bump": [
        "npm run build"
      ]
    },
    "npm": {
      "publishPath": "./dist"
    },
    "git": {
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": true
    }
  }
}
